<body>
    <nav class="navbar navbar-dark bg-dark fixed-top">
        <div class="container-fluid">
          <a class="navbar-brand" href="/">        
            <img src="../assets/flyming_icon.png" class="icon">
            <h1 class="title">Flyming Photography Page</h1>
          </a>
          <!-- normal nav bar -->
          <ul class="nav-menu">
            <li class="Home nav-item"><a href="#" class="nav-link active">首頁</a></li>
            <li class="About nav-item"><a href="/aboutme" class="nav-link">關於我</a></li>
            <li class="Project nav-item"><a href="/project" class="nav-link">作品</a></li>
            <li class="contact nav-item"><a href="/contact" class="nav-link">聯絡我</a></li>
          </ul>
          <!-- collapse nav bar -->
          <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasDarkNavbar" aria-controls="offcanvasDarkNavbar" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon custom-toggler-icon">
                <i class="fa-solid fa-bars fa-lg"></i>
            </span>
          </button>
          <div class="offcanvas offcanvas-end text-bg-dark" tabindex="-1" id="offcanvasDarkNavbar" aria-labelledby="offcanvasDarkNavbarLabel">
            <div class="offcanvas-header">
              <button type="button" class="btn-close btn-close-black" data-bs-dismiss="offcanvas" aria-label="Close"></button>
            </div>
            <div class="offcanvas-body">
              <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
                <li class="Home nav-item"><a href="#" class="nav-link active">首頁</a></li>
                <li class="About nav-item"><a href="/aboutme" class="nav-link">關於我</a></li>
